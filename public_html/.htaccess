	php_value	session.gc_divisor			500
	php_value	session.gc_probability		1
</IfModule>
<IfModule mod_headers.c>
  <IfModule mod_env.c>
	Header unset Server
	Header unset X-Powered-By
    SetEnv modHeadersAvailable true
  </IfModule>
</IfModule>
<ifModule ModSecurity.c>
  SecServerSignature ''
</ifModule>
<IfModule mod_dir.c>
  DirectoryIndex index.php
</IfModule>
